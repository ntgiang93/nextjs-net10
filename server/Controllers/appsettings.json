{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "ConnectionStrings": {
    "MainDb": "Server=localhost,1433;Database=NextDotNet;User ID=sa;Password=yourStrong(!)Password;MultipleActiveResultSets=True;TrustServerCertificate=True;"
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console"
    ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "Console"
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ],
    "Properties": {
      "Application": "MyAppName"
    }
  },
  "AppSettings": {
    "FileDomain": "https://localhost:7078",
    "VerificationType": 1,
    "Cors": [
      "https://localhost:5173",
      "http://localhost:5173",
      "https://localhost:3000",
      "http://localhost:3000"
    ],
    "Jwt": {
      "Issuer": "http://localhost:5173",
      "Audience": "http://localhost:5173",
      "SingingKey": "$3!kP#r2^Lq@v9&yFgXwNzTb8Uj*JhV5mDchfyresds845HGYR9843hhdsu!@hre83uDFG0HD",
      "TokenExpiresIn": 15,
      "RefreshTokenExpiresIn": 7
    },
    "EmailConfiguration": {
      "Provider": "SMTP",
      "SMTP": {
        "Host": "smtp.example.com",
        "Port": 587,
        "UserName": "your-username",
        "Password": "your-password",
        "EnableSsl": true,
        "FromEmail": "noreply@yourapp.com",
        "FromName": "NextDotNet App"
      }
    }
  },
  "AllowedHosts": "*"
}
